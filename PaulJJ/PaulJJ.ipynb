    "def calculate_mean(numbers):\n",
    "        \n",
    "\n",
    "if not numbers:\n",
    "            raise ValueError(\"Cannot calculate mean of an empty list\")\n",
    "total = sum(numbers)\n",
    "count = len(numbers)\n",
    "mean = total / count\n",
    "return mean"
   ]
  }
